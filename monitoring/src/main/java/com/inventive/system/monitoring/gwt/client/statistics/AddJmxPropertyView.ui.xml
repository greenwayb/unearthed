<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <ui:style>

        .left {
            float: left;
        }

        .leftPanel {
            width: 200px;
        }

    </ui:style>

    <g:HTMLPanel ui:field="rootPanel">

        <div>
            <g:Label text="Object Name:" addStyleNames="{style.left} {style.leftPanel}"/>
            <g:TextBox ui:field="objectNameTextBox" addStyleNames="{style.left}" text="java.lang:type=Memory"/>
        </div>
        <div>
            <g:Label text="Property Name:" addStyleNames="{style.left} {style.leftPanel}"/>
            <g:TextBox ui:field="propertyNameTextBox" addStyleNames="{style.left}" text="HeapMemoryUsage"/>
        </div>
        <div>
            <g:Label text="Machine Name:" addStyleNames="{style.left} {style.leftPanel}"/>
            <g:TextBox ui:field="machineNameTextBox" addStyleNames="{style.left}" text="Grants-MacBook-Pro-Retina.local"/>
        </div>
        <div>
            <g:Label text="Process Name:" addStyleNames="{style.left} {style.leftPanel}"/>
            <g:TextBox ui:field="processNameTextBox" addStyleNames="{style.left}"/>
        </div>
        <div>
            <g:Label text="Field Name:" addStyleNames="{style.left} {style.leftPanel}"/>
            <g:TextBox ui:field="fieldNameTextBox" addStyleNames="{style.left}" text="Used"/>
        </div>
        <g:Button ui:field="okButton" text="OK" />
        <g:Button ui:field="cancelButton" text="Cancel" />
    </g:HTMLPanel>
</ui:UiBinder>